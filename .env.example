# Database (Supabase PostgreSQL)
DATABASE_URL="postgresql://postgres.PROJECT_REF:password@aws-1-ap-southeast-1.pooler.supabase.com:5432/postgres"

# Supabase
SUPABASE_URL="https://PROJECT_REF.supabase.co"
SUPABASE_ANON_KEY="your_supabase_anon_key"

# Server
PORT=3000
NODE_ENV=development

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379

# Booking.com API
BOOKING_API_URL=https://api.booking.com/v1
BOOKING_API_KEY=your_booking_api_key
BOOKING_API_SECRET=your_booking_api_secret

# Yanolja API
YANOLJA_API_URL=https://api.yanolja.com/v1
YANOLJA_API_KEY=your_yanolja_api_key
YANOLJA_API_SECRET=your_yanolja_api_secret

# Airbnb API
AIRBNB_API_URL=https://api.airbnb.com/v2
AIRBNB_CLIENT_ID=your_airbnb_client_id
AIRBNB_CLIENT_SECRET=your_airbnb_client_secret
AIRBNB_ACCESS_TOKEN=your_airbnb_access_token

# Sync Settings
SYNC_INTERVAL_MINUTES=5
